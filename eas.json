{
  "cli": {
    "version": ">= 16.23.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "prebuildCommand": "node scripts/setup-google-services.js"
    },
    "preview": {
      "distribution": "internal",
      "prebuildCommand": "node scripts/setup-google-services.js"
    },
    "production": {
      "autoIncrement": true,
      "prebuildCommand": "node scripts/setup-google-services.js"
    },
    
    "production-apk": {
      "distribution": "internal",
        "android": {
          "buildType": "apk"
        },
      "prebuildCommand": "node scripts/setup-google-services.js"
    }
  },
  "submit": {
    "production": {}
  }
}
